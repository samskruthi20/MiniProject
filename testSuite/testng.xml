<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

<listeners>
<listener class-name="seleniumFramework.testComponents.Listeners"/>
</listeners>


  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="seleniumFramework.testScripts.SubmitOrderTest"/>
</classes>
</test> 

<test thread-count="5" name="Error Validations Tests">
    <classes>
      <class name="seleniumFramework.testScripts.ErrorValidationsTest"/>
</classes>
</test> 


</suite>


 